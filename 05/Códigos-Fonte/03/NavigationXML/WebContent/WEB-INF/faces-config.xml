<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<navigation-rule>
		<display-name>page_a.xhtml</display-name>
		<from-view-id>/page_a.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>b</from-outcome>
			<to-view-id>/page_b.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>page_b.xhtml</display-name>
		<from-view-id>/page_b.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>a</from-outcome>
			<to-view-id>/page_a.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
</faces-config>
